;; Allow you to use M-up and M-down to move a line up or down
;; (drag-stuff-define-keys)
;; (drag-stuff-global-mode 1)
(global-set-key [(meta up)] 'drag-stuff-up)
(global-set-key [(meta down)] 'drag-stuff-down)
(global-set-key [(meta right)] 'drag-stuff-right)
(global-set-key [(meta left)] 'drag-stuff-left)
